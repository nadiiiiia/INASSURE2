{% extends '::default/home.html.twig' %}
{% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('am/lib/stroke-7/style.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('am/lib/jquery.nanoscroller/css/nanoscroller.css') }}"/><!--[if lt IE 9]><![endif]-->
        <link rel="stylesheet" type="text/css" href="{{ asset('am/lib/morrisjs/morris.css') }}"/>
        <link rel="stylesheet"  href="{{ asset('am/css/style.css') }}" type="text/css"/>

{% endblock %}
{% block login %}
    <div>
        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
            {#{{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |#}
            {#<a href="{{ path('fos_user_security_logout') }}">#}
                {#{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}#}
            </a>
        {% else %}
            <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
        {% endif %}
    </div>
    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="flash-{{ type }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    <div>
        {% block fos_user_content %}
        {% endblock fos_user_content %}
    </div>

{% endblock %}
{% block javascripts %}
        <script src="{{ asset('am/lib/jquery/jquery.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('am/lib/jquery.nanoscroller/javascripts/jquery.nanoscroller.min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('am/js/main.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('am/lib/bootstrap/dist/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('am/lib/raphael/raphael-min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('am/lib/morrisjs/morris.min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('am/js/app-charts-morris.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('js/delete.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('js/edit.js') }}"  type="text/javascript"></script>
    {% endblock %}