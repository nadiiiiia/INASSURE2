<!DOCTYPE html>
<html>
<head>
    {% block stylesheets %}
        <link href="{{ asset('bootstrap/css/bootstrap.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/style.css') }}" rel="stylesheet" />
    {% endblock %}
    {% block javascripts %}
        <script type="application/x-javascript"> addEventListener("load",
                    function () {
                        setTimeout(hideURLbar, 0);
                    }, false);
            function hideURLbar() {
                window.scrollTo(0, 1);
            }
        </script>
        <script src="{{ asset('plugins/modernizr.js') }}"type="text/javascript" ></script>
        <script src="{{ asset('plugins/jquery.browser.js') }}"type="text/javascript" ></script>
    {% endblock %}
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>

</head>
<body>
<div class="container">
    {% block login %}
    {% endblock %}


</div>
</body>
</html>